#include <stdlib.h>
#include <string.h>
#include <stdio.h>
#include <stdbool.h>
#include <err.h>
#include <SDL2/SDL.h>
#include <SDL2/SDL_image.h>
#include "postprocess.c"

void conv(char *path)
{
    SDL_Surface *image = postprocess(path);
    IMG_SavePNG(image, "partition.png");
    SDL_FreeSurface(image);
    SDL_Quit();
}
